# Zoom App credentials (from Zoom Marketplace â†’ your General App)
ZOOM_CLIENT_ID=
ZOOM_CLIENT_SECRET=
ZOOM_WEBHOOK_SECRET_TOKEN=

# Moveris API key (from https://documentation.moveris.com)
MOVERIS_API_KEY=

# Analysis mode: "fast" (10 crops, one-time check) or "live" (repeat checks)
MOVERIS_MODE=fast

# Process every Nth decoded frame (5 = every 5th frame from ~30fps stream)
FRAME_SAMPLE_RATE=5

# Liveness score threshold: score >= this value = PASS
# Per Moveris API docs, the threshold is 65 (https://documentation.moveris.com/api-reference/fast-check)
LIVENESS_THRESHOLD=65

# Max simultaneous meeting sessions handled by this instance
MAX_CONCURRENT_SESSIONS=50

LOG_LEVEL=INFO
